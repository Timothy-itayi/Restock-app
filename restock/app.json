{
  "expo": {
    "name": "restock",
    "slug": "restock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/restock-icon.png",
    "scheme": "restock",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timothyitayi.restock",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/restock-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.timothyitayi.restock"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/restock-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/restock-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fafafa"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Satoshi/Satoshi-Black.otf",
            "./assets/fonts/Satoshi/Satoshi-BlackItalic.otf",
            "./assets/fonts/Satoshi/Satoshi-Bold.otf",
            "./assets/fonts/Satoshi/Satoshi-BoldItalic.otf",
            "./assets/fonts/Satoshi/Satoshi-Medium.otf",
            "./assets/fonts/Satoshi/Satoshi-MediumItalic.otf",
            "./assets/fonts/Satoshi/Satoshi-Regular.otf",
            "./assets/fonts/Satoshi/Satoshi-Italic.otf",
            "./assets/fonts/Satoshi/Satoshi-Light.otf",
            "./assets/fonts/Satoshi/Satoshi-LightItalic.otf"
          ]
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0641bf2c-7f00-4f4f-b5a2-7552036f4fda"
      }
    }
  }
}
